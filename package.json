{
  "name": "aefinder",
  "version": "2.17.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:mainnet": "NEXT_PUBLIC_NETWORK_KEY=mainnet next build",
    "build:testnet": "NEXT_PUBLIC_NETWORK_KEY=testnet next build",
    "build:devnet": "NEXT_PUBLIC_NETWORK_KEY=devnet next build",
    "build:localnet": "NEXT_PUBLIC_NETWORK_KEY=localnet next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "typecheck": "tsc --noEmit --incremental false",
    "test:watch": "jest --watch",
    "test": "jest",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.20.0",
    "@ant-design/nextjs-registry": "^1.0.0",
    "@reduxjs/toolkit": "^2.2.5",
    "antd": "^5.17.4",
    "axios": "^0.24.0",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.11",
    "dexie": "^4.0.8",
    "graphiql": "^3.3.1",
    "graphql": "^16.9.0",
    "lucide-react": "^0.260.0",
    "next": "14.0.1",
    "next-compose-plugins": "^2.2.1",
    "pako": "^2.1.0",
    "qs": "^6.12.3",
    "query-string": "^9.0.0",
    "react": "18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.10.1",
    "react-json-view": "^1.21.3",
    "react-redux": "^9.1.2",
    "react-use": "^17.5.0",
    "tailwind-merge": "^1.13.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@svgr/webpack": "^6.5.1",
    "@tailwindcss/forms": "^0.5.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-config-next": "^13.4.10",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.5.0",
    "next-router-mock": "^0.7.5",
    "next-sitemap": "^2.5.28",
    "postcss": "^8.4.26",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "**/*.{json,css,scss,md,webmanifest}": [
      "prettier -w"
    ]
  }
}
